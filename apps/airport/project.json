{
  "name": "airport",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/airport/src",
  "projectType": "application",
  "prefix": "airport",
  "generators": {},
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "options": {
        "outputPath": "dist/apps/airport",
        "main": "apps/airport/src/main.ts",
        "tsConfig": "apps/airport/tsconfig.app.json",
        "assets": ["apps/airport/src/assets"],
        "target": "node",
        "compiler": "tsc"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/airport/src/environments/environment.ts",
              "with": "apps/airport/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "airport:build",
        "inspect": false
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/airport/**/*.ts", "apps/airport/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "apps/airport/jest.config.ts"
      }
    }
  },
  "tags": []
}
